			gpio-controller;
			#gpio-cells = <2>;

			interrupt-controller;
			#interrupt-cells = <2>;
		};

		jtag_trst_n: jtag_trst_n {
			samsung,pins = "etc0-0";
			samsung,pin-function = <0x2>;
		};

		jtag_tms: jtag_tms {
			samsung,pins = "etc0-1";
			samsung,pin-function = <0x2>;
		};

		jtag_tck: jtag_tck {
			samsung,pins = "etc0-2";
			samsung,pin-function = <0x2>;
		};

		jtag_tdi: jtag_tdi {
			samsung,pins = "etc0-3";
			samsung,pin-function = <0x2>;
		};

		jtag_tdo: jtag_tdo {
			samsung,pins = "etc0-4";
			samsung,pin-function = <0x2>;
		};
