	/* I2C_MOTOR */
	i2c@13880000 {
		status = "okay";
		#address-cells = <1>;
		#size-cells = <0>;

		drv2624@5A{
			compatible = "drv2624";
			reg = <0x5A>;
		};
	};

    i2c@13890000 {
		status = "okay";
		#address-cells = <1>;
		#size-cells = <0>;
		gpios = <&gpc2 0 0 /* sda */
				&gpc2 1 0 >; /* scl */
		i2c-gpio,delay-us = <10>;
		i2c-gpio,timeout-ms = <0>;

		sec-nfc@2B{
			compatible = "sec-nfc,i2c";
			reg = <0x2B>;
			pinctrl-names = "default";
			pinctrl-0 = <&nfc_irq &gpio_nfc_en &gpio_nfc_firm>;
			sec-nfc,irq-gpio = <&gpa0 6 0xf>;
			sec-nfc,ven-gpio = <&gpc2 2 0x1>;
			sec-nfc,firm-gpio = <&gpd1 0 0x00>;
			sec-nfc,gpio-sda = <&gpc2 0 0x00>;
			sec-nfc,gpio-scl = <&gpc2 1 0x00>;
		};
	};

	pinctrl@139F0000 {
		wpc_irq: wpc_irq {
			samsung,pins = "gpa0-7";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <0>;
		};
	};

    i2c@13830000 {
		status = "okay";
		#address-cells = <1>;
		#size-cells = <0>;
		gpios = <&gpc4 0 0 /* sda */
				&gpc4 1 0 >; /* scl */
		i2c-gpio,delay-us = <10>;
		i2c-gpio,timeout-ms = <0>;

		p9220-charger@61{
			compatible = "p9220,i2c";
			reg = <0x61>;
			pinctrl-names = "default";
			pinctrl-0 = <&wpc_irq>;
			interrupts = <7 0 0>;
			interrupt-parent = <&gpa0>;
			p9220,irq-gpio = <&gpa0 7 0x00>;
			p9220,gpio-sda = <&gpc4 0 0x00>;
			p9220,gpio-scl = <&gpc4 1 0x00>;

			p9220,dedicated-tx-type = <3>;		/* TX_TYPE_S3 */
								/* (vrec, vout)  */
			p9220,sec_mode_data = <	0	0		/* SEC_INITIAL_MODE 		*/
									110	4900	/* SEC_SWELLING_MODE 		*/
									15	4600	/* SEC_LOW_BATT_MODE		*/
									15	4900	/* SEC_MID_BATT_MODE		*/
									70	4900	/* SEC_HIGH_BATT_MODE		*/
									94	4900	/* SEC_VERY_HIGH_BATT_MODE	*/
									94	4900	/* SEC_FULL_NONE_MODE		*/
									110	4900	/* SEC_FULL_NORMAL_MODE		*/
									62	4600	/* SEC_SWELLING_LDU_MODE	*/
									62	4600	/* SEC_LOW_BATT_LDU_MODE	*/
									126	4900	/* SEC_FULL_NONE_LDU_MODE 	*/
									62	4600	/* SEC_FULL_NORMAL_LDU_MODE */
									>;
		};
	};

